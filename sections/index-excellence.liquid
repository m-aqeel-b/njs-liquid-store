{{ 'index-excellence.css' | asset_url | stylesheet_tag }}

<div class="hb_excellence">
  <div class="hb_main_container">
    <div class="hb_wrapper">
      <h2 class="hb_excellence--title">
        {{ section.settings.title }}
      </h2>
      <p class="hb_excellence--p">
        {{ section.settings.description }}
      </p>
    </div>
  </div>
</div>
{% schema %}
  {
    "name": "index excellence",
    "settings": [
      {
        "type": "text",
        "id": "title",
        "label": "Title"
      }, {
        "type": "text",
        "id": "description",
        "label": "Description"
      }
    ],
    "presets": [
      {
        "name": "index excellence"
      }
    ]
  }
{% endschema %}