{{ 'testimonials.css' | asset_url | stylesheet_tag }}
<div class="hb_tabs">
  <div class="hb_main_container">
    <div class="tab">
      <button class="tablinks active" onclick="openCity(event, 'google_Reviews')">
        Google Reviews
      </button>
      <button class="tablinks" onclick="openCity(event, 'testimonials')">
        Testimonials
      </button>
      <button class="tablinks" onclick="openCity(event, 'video_testimonials')">
        Video Testimonials
      </button>
    </div>
    <hr class="hb_line hb_line__testimonials" />

    <!-- Tab content -->
    <div
      id="google_Reviews"
      class="tabcontent"
      style="display: block">
      <div class="hb_testimonial--list">
        {% for block in section.blocks %}
          {% if block.type == 'google_reivews' %}
            <div class="hb_testi--item">
              <p class="hb_testim--p">
                {{ block.settings.google_review }}
              </p>
              <div class="hb_testim_item--footer">
                <h3 class="hb_testim--name">
                  {{ block.settings.name }}</h3>
              </div>
            </div>
          {% endif %}
        {% endfor %}


      </div>
    </div>

    <div id="testimonials" class="tabcontent">
      <div class="hb_comments__list">
