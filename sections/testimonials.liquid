      </div>
    </div>

    <div id="video_testimonials" class="tabcontent">
      <div class="hb_videos__list">
        {% for block in section.blocks %}
          {% if block.type == 'video_testimonial' %}
            <div class="hb_video--item">
              <img
                src="{{ block.settings.video_img |  image_url: width:'500x'  }}"
                alt=""
                class="hb_video_img--style" />
              <h3 class="hb_video_item_name">
                {{ block.settings.name }}
              </h3>
            </div>
          {% endif %}
        {% endfor %}


      </div>
    </div>
